<div class="y-player-mask"></div><div class="y-player-video-wrap">{{ include './video.art' video }}{{ if options.logo }}<div class="y-player-logo"><img src="{{ options.logo }}"></div>{{ /if }}<div class="y-player-bezel"><span class="y-player-bezel-icon"></span><span class="y-player-loading-icon">{{@ icons.loading }}</span></div></div><div class="y-player-controller-mask"></div><div class="y-player-controller"><div class="y-player-icons y-player-icons-left"><button class="y-player-icon y-player-play-icon"><span class="y-player-icon-content">{{@ icons.play }}</span></button><div class="y-player-volume"><button class="y-player-icon y-player-volume-icon"><span class="y-player-icon-content">{{@ icons.volumeDown }}</span></button><div class="y-player-volume-bar-wrap" data-balloon-pos="up"><div class="y-player-volume-bar"><div class="y-player-volume-bar-inner" style="background: {{ options.theme.volume }};"><span class="y-player-thumb" style="background: {{ options.theme.volume }}"></span></div></div></div></div><span class="y-player-time"><span class="y-player-ptime">00:00</span> / <span class="y-player-dtime">00:00</span></span></div><div class="y-player-icons y-player-icons-right">{{ if options.video.quality }}<div class="y-player-quality"><button class="y-player-icon y-player-quality-icon">{{ options.video.quality[options.video.defaultQuality].name }}</button><div class="y-player-quality-mask"><div class="y-player-quality-list">{{ each options.video.quality }}<div class="y-player-quality-item" data-index="{{ $index }}">{{ $value.name }}</div>{{ /each }}</div></div></div>{{ /if }}<div class="y-player-speed-down"><button class="y-player-icon y-player-speed-down-icon" data-balloon="{{ tran('Decrease speed') }}" data-balloon-pos="up"><span class="y-player-icon-content">{{@ icons.speedDown}}</span></button></div><div class="y-player-speed"><button class="y-player-icon y-player-speed-icon" data-balloon="{{ tran('Speed') }}" data-balloon-pos="up">1.00&times;</button></div><di